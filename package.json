{
  "name": "ca-ev-dashboard",
  "description": "Dashboard for Electric Vehicle charging station",
  "homepage": "https://github.com/charge-angels/ca-ev-dashboard",
  "version": "2.7.4",
  "engines": {
    "node": "^16.15.x",
    "npm": "^8.11.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charge-angels/ca-ev-dashboard.git"
  },
  "author": "Charge Angels <cloud@chargeangels.fr>",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs": {
      "parser": "babylon",
      "style": "module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "scripts": {
    "start": "npm version && ng serve --configuration=development --poll 10",
    "start:ssl": "npm version && ng serve --configuration=development --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "start:prod": "npm version && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --configuration production",
    "start:prod:ssl": "npm version && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --configuration production --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "build": "npm run build:dev",
    "build:prod": "npm version && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng build --configuration production",
    "build:prod:stats": "npm version && cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng build --configuration production --stats-json",
    "build:dev": "npm version && ng build --configuration=development --stats-json",
    "git:sinit": "git submodule update --init --recursive --force",
    "git:sdiff": "git diff && git submodule foreach 'git diff'",
    "git:supdate": "git submodule update --remote --recursive --merge",
    "git:spush": "git push --recurse-submodules=on-demand",
    "git:sreset": "git submodule foreach 'git reset --hard'",
    "eslint": "ng lint",
    "eslint:fix": "ng lint --fix=true",
    "eslint:html": "npm run eslint:file && npm run eslint:report",
    "eslint:file": "mkdir -p test/results-eslint && ng lint --format json --silent > test/results-eslint/jsonOutput.json",
    "eslint:report": "ng-lint-report --jsonfile test/results-eslint/jsonOutput.json --output test/results-eslint",
    "clean-install": "rimraf package-lock.json && rimraf node_modules && npm cache clear --force && npm install --force",
    "npm-check": "npm-check --skip-unused",
    "ng:update": "ng update",
    "angular-update": "ng update @angular/cli @angular/core",
    "material-update": "ng update @angular/material",
    "angular11-update": "ng update @angular/core@11 @angular/cli@11",
    "material11-update": "ng update @angular/material@11",
    "webpack-analyze": "webpack-bundle-analyzer dist/stats.json",
    "import-sort": "npx import-sort-cli --write '{src,test}/**/*.ts{,x}'",
    "check:i18n": "cross-env TS_NODE_FILES=true ts-node-dev --project tsconfig-i18n.json --files test/I18nChecker.ts",
    "test": "npm version && cross-env TS_NODE_FILES=true jest --config='jest.config.ts'  --reporters='default' --reporters='./test/JestEvseReporter' --runInBand --logHeapUsage --verbose --colors"
  },
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^14.2.6",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/google-maps": "^14.2.6",
    "@angular/localize": "^14.2.8",
    "@angular/material": "^14.2.6",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@auth0/angular-jwt": "^5.1.0",
    "@googlemaps/markerclusterer": "^2.0.13",
    "@ng-bootstrap/ng-bootstrap": "^13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@stripe/stripe-js": "^1.44.1",
    "bootstrap": "^5.2.2",
    "bootstrap-material-design": "^4.1.3",
    "bootstrap-notify": "^3.1.3",
    "bootstrap-select": "^1.13.18",
    "chart.js": "^4.0.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.6",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "http-status-codes": "^2.2.0",
    "jasny-bootstrap": "^4.0.0",
    "jquery": "^3.6.1",
    "ng-pick-datetime": "^7.0.0",
    "ng2-file-upload": "^1.4.0",
    "ngx-captcha": "^12.0.2",
    "ngx-csv-parser": "^1.2.0",
    "ngx-google-places-autocomplete": "^2.0.5",
    "popper.js": "^1.16.1",
    "rxjs": "^7.5.7",
    "tldts": "^5.7.97",
    "ts-jest": "^29.0.3",
    "tslib": "^2.4.1",
    "zone.js": "~0.12.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^5.0.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.8",
    "@angular/compiler-cli": "^14.2.10",
    "@types/bootstrap": "^5.2.5",
    "@types/chart.js": "^2.9.37",
    "@types/convict": "^6.1.1",
    "@types/expect-puppeteer": "^5.0.2",
    "@types/file-saver": "^2.0.5",
    "@types/google.maps": "^3.50.5",
    "@types/jest": "^29.2.1",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/jquery": "^3.5.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^16.11.68",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.42.0",
    "convict": "^6.2.3",
    "cross-env": "^7.0.3",
    "deep-object-diff": "^1.1.9",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "^39.6.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.1",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.2.2",
    "jest-puppeteer": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment-locales-webpack-plugin": "^1.2.0",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "ng-lint-report": "^0.2.0",
    "npm-check": "^6.0.1",
    "puppeteer": "^19.2.1",
    "rimraf": "^3.0.2",
    "rosie": "^2.1.0",
    "ts-node": "~10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0"
  }
}
